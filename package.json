{
  "name": "express-typescript-server",
  "version": "1.0.0",
  "description": "Production-ready Express server with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "prisma:generate": "npx prisma generate",
    "prisma:deploy": "npx prisma migrate deploy",
    "build": "npm run prisma:generate && tsc && tsc-alias && npm run copy:prisma && npm run postbuild",
    "build:render": "npm run prisma:generate && npm run prisma:deploy && tsc && tsc-alias && npm run copy:prisma && npm run postbuild",
    "copy:prisma": "mkdir -p dist/generated && cp -r src/generated/prisma dist/generated/",
    "postbuild": "node scripts/fix-imports.js",
    "start": "node dist/index.js",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "db:setup": "ts-node -r tsconfig-paths/register src/scripts/setupDatabase.ts",
    "approve-candidate": "ts-node -r tsconfig-paths/register scripts/approveCandidate.ts",
    "seed:templates": "ts-node -r tsconfig-paths/register scripts/seed-notification-templates.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "express",
    "typescript",
    "api",
    "server"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.19.0",
    "@supabase/storage-js": "^2.86.0",
    "@supabase/supabase-js": "^2.78.0",
    "@types/node-cron": "^3.0.11",
    "@types/papaparse": "^5.5.1",
    "@types/pdfkit": "^0.17.4",
    "@types/yamljs": "^0.2.34",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dompurify": "^3.3.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "isomorphic-dompurify": "^2.30.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.11",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "prisma": "^6.19.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/csv-parse": "^1.1.12",
    "@types/dompurify": "^3.0.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsdom": "^27.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^7.0.4",
    "@types/pg": "^8.10.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
